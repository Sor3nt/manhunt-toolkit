#!/usr/bin/env php
<?php
// application.php

require __DIR__.'/vendor/autoload.php';

use Symfony\Component\Console\Application;

$application = new Application();

// ... register commands


$command = new \App\Command\PackCommand(new \App\Service\Archive\Mls(), new \App\Service\Archive\Glg(), new \App\Service\Archive\Inst());
$application->add($command);

$command = new \App\Command\UnpackCommand(new \App\Service\Archive\Mls(), new \App\Service\Archive\Glg(), new \App\Service\Archive\Inst());
$application->add($command);

$application->run();